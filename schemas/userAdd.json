{
  "$schema": "https://json-schema.org/draft/2019-09/schema",
  "$id": "http://example.com/example.json",
  "type": "object",
  "default": {},
  "title": "Root Schema",
  "required": [
    "username",
    "password",
    "firstName",
    "lastName",
    "email",
    "accountBalance"
  ],
  "properties": {
    "username": {
      "type": "string",
      "default": "",
      "minLength": 1,
      "examples": ["testuser2"]
    },
    "password": {
      "type": "string",
      "default": "",
      "minLength": 6,
      "examples": ["pass"]
    },
    "firstName": {
      "type": "string",
      "default": "",
      "minLength": 1,
      "examples": ["Test"]
    },
    "lastName": {
      "type": "string",
      "default": "",
      "minLength": 1,
      "examples": ["User"]
    },
    "email": {
      "type": "string",
      "default": "",
      "minLength": 1,
      "format": "email",
      "examples": ["testuser1@email.com"]
    },
    "accountBalance": {
      "type": "number",
      "default": 0,
      "minimum": 0,
      "examples": [10000]
    },
    "roles": {
      "type": "array",
      "default": [],
      "items": {
        "type": "string",
        "examples": ["admin", "investor"]
      }
    },
    "examples": [
      {
        "username": "testuser2",
        "password": "pass",
        "firstName": "Test",
        "lastName": "User",
        "email": "testuser1@email.com",
        "accountBalance": 10000
      }
    ]
  }
}
