{
  "$schema": "https://json-schema.org/draft/2019-09/schema",
  "$id": "http://example.com/example.json",
  "type": "object",
  "title": "Approve Loan Application Schema",
  "required": ["amtApproved", "interestRate", "durationMonths"],
  "properties": {
    "amtApproved": {
      "type": "number",
      "minimum": 1
