{
  "$schema": "https://json-schema.org/draft/2019-09/schema",
  "$id": "http://example.com/example.json",
  "type": "object",
  "title": "New Loan Application Schema",
  "required": ["amtRequested", "purposeId", "term"],
  "properties": {
    "amtRequested": {
      "type": "number",
      "minimum": 1
    },
    "purposeId": {
      "type": "integer"
    },
    "interestRate": {
      "type": "number",
      "minimum": 0.0001
    },
    "term": {
      "type": "integer",
      "minimum": 1
    }
  }
}
